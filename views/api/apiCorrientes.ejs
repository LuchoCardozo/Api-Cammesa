<%- include('../partials/head',{title:'Corrientes'}); %>
<body>
    <%- include('../partials/header') %>
<main>


    <h1>DEMANDA CORRIENTES</h1>
    <div class="grafico" >
        <canvas id="myChart" ></canvas>
        </div>
    <div class="tabla">
        <table >
            <thead>
                <tr>
                    <th>Fecha</th>
                    <th>demHoy</th>
                    <th>demAyer</th>
                    <th>demSemAnt</th>
                </tr>
            </thead>
            
            <tbody>
                <% corrientes.forEach(dat => { %>
                    <tr>
                        <td><%= dat.fecha %></td>
                        <td><%= dat.demHoy %></td>
                        <td><%= dat.demAyer %></td>
                        <td><%= dat.demSemanaAnt %></td>
                    </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
    </main>
     <script src="/js/grafCorrientes.js"></script>
</body>
</html>