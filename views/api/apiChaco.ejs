<%- include('../partials/head',{title:'Chaco'}) %>

<body>
    <%- include('../partials/header') %>
    <h1>DEMANDA CHACO</h1>
            <div class="grafico" >
            <canvas id="myChart" ></canvas>
            </div>
                <div class="tabla">
                    <table>
                        <thead>
                            <tr>
                                <th>Fecha</th>
                                <th>demHoy</th>
                                <th>demAyer</th>
                                <th>demSemAnt</th>
                            </tr>
                        </thead>

                        <tbody>
                            <% chaco.forEach(dat=> { %>
                                <tr>
                                    <td>
                                        <%= dat.fecha %>
                                    </td>
                                    <td>
                                        <%= dat.demHoy %>
                                    </td>
                                    <td>
                                        <%= dat.demAyer %>
                                    </td>
                                    <td>
                                        <%= dat.demSemanaAnt %>
                                    </td>
                                </tr>
                                <% }) %>
                        </tbody>
                    </table>
                </div>

<script src="/js/grafChaco.js"></script>
</body>

</html>